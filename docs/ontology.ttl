###  http://www.semanticweb.org/abolance/ontologies/2025/3/CityExplorer#AndalusianRestaurant
<http://www.semanticweb.org/abolance/ontologies/2025/3/CityExplorer#AndalusianRestaurant> rdf:type owl:Class ;
                                                                                          owl:equivalentClass [ owl:intersectionOf ( <http://www.semanticweb.org/abolance/ontologies/2025/3/CityExplorer#Restaurant>
                                                                                                                                     [ rdf:type owl:Restriction ;
                                                                                                                                       owl:onProperty <http://www.semanticweb.org/abolance/ontologies/2025/3/CityExplorer#hasCategory> ;
                                                                                                                                       owl:hasValue <http://www.semanticweb.org/abolance/ontologies/2025/3/CityExplorer#Andalusian>
                                                                                                                                     ]
                                                                                                                                   ) ;
                                                                                                                rdf:type owl:Class
                                                                                                              ] ;
                                                                                          rdfs:subClassOf <http://www.semanticweb.org/abolance/ontologies/2025/3/CityExplorer#RestaurantCategory> ;
                                                                                          rdfs:comment "Refers to restaurants serving food from Andalusia"@en ;
                                                                                          rdfs:label "AndalusianRestaurant"@en .


###  http://www.semanticweb.org/abolance/ontologies/2025/3/CityExplorer#Attraction
<http://www.semanticweb.org/abolance/ontologies/2025/3/CityExplorer#Attraction> rdf:type owl:Class ;
                                                                                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                                                  owl:onProperty <http://www.semanticweb.org/abolance/ontologies/2025/3/CityExplorer#locatedInCity> ;
                                                                                                  owl:someValuesFrom <http://www.semanticweb.org/abolance/ontologies/2025/3/CityExplorer#City>
                                                                                                ] ,
                                                                                                [ rdf:type owl:Restriction ;
                                                                                                  owl:onProperty <http://www.semanticweb.org/abolance/ontologies/2025/3/CityExplorer#hasCategory> ;
                                                                                                  owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                                                  owl:onClass <http://www.semanticweb.org/abolance/ontologies/2025/3/CityExplorer#AttractionTypes>
                                                                                                ] ,
                                                                                                [ rdf:type owl:Restriction ;
                                                                                                  owl:onProperty <http://www.semanticweb.org/abolance/ontologies/2025/3/CityExplorer#aggregateRating> ;
                                                                                                  owl:someValuesFrom xsd:decimal
                                                                                                ] ,
                                                                                                [ rdf:type owl:Restriction ;
                                                                                                  owl:onProperty <http://www.semanticweb.org/abolance/ontologies/2025/3/CityExplorer#identifier> ;
                                                                                                  owl:someValuesFrom xsd:string
                                                                                                ] ,
                                                                                                [ rdf:type owl:Restriction ;
                                                                                                  owl:onProperty <http://www.semanticweb.org/abolance/ontologies/2025/3/CityExplorer#name> ;
                                                                                                  owl:someValuesFrom xsd:string
                                                                                                ] ;
                                                                                rdfs:comment "Refers to an attraction type, which can be cultural, park, etc."@en ;
                                                                                rdfs:label "Attraction"@en ;
                                                                                rdfs:sameAs "https://schema.org/Attraction"@en .


